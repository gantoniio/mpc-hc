msgid "Input"
msgstr "입력"

msgctxt "IDS_ARS_OUTPUT"
msgid "Output"
msgstr "출력"

msgctxt "IDS_ARS_FORMAT"
msgid "Format:"
msgstr "형식 :"

msgctxt "IDS_ARS_SAMPLERATE"
msgid "Sample rate:"
msgstr "표본화 :"

msgctxt "IDS_ARS_CHANNELS"
msgid "Channels:"
msgstr "채널 :"

msgctxt "IDS_ARS_WASAPI_METHOD"
msgid "WASAPI method:"
msgstr "WASAPI 메소드 :"

msgctxt "IDS_ARS_DUMMY_CHANNELS"
msgid "Don't fill channels when upmixing"
msgstr "업믹싱 시, 채널을 채우지 않음"

msgctxt "IDS_ARS_TIP_BITEXACT_OUTPUT"
msgid ""
"If possible, the audio will be sent out in its original format.\n"
"Otherwise, the format specified in the system settings will be used."
